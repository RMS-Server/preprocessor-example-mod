{
  "schemaVersion": 1,
  "id": "$id",
  "version": "$version",
  "name": "Example Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://github.com/RMS-Server/preprocessor-example-mod",
    "sources": "https://github.com/RMS-Server/preprocessor-example-mod"
  },
  "license": "GPL-3.0",
  "icon": "assets/$id/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.example.ExampleMod"
    ]
  },
  "mixins": [
    "${id}.mixins.json"
  ],
  "accessWidener": "${id}.accesswidener",
  "depends": {
    "fabricloader": ">=$loader_version",
    "minecraft": "$minecraft_version"
  }
}
